    <div class=\"page-wrap\">\r\n            <titlebar :add=\"add\" :search=\"search\" :leftTitle=\"leftTitle\" :rightFirstImg=\"rightFirstImg\" :rightSecondImg=\"rightSecondImg\"></titlebar> \r\n            <!-- div保留(标题)48px高度 -->\r\n            <div style=\"height:48px\"></div>\r\n            <!-- 3.切换面板列表 -->\r\n            <mt-tab-container class=\"page-tabbar-container\" v-model=\"active\">\r\n                <mt-tab-container-item id=\"message\">\r\n                    <messagelist></messagelist>\r\n                </mt-tab-container-item >\r\n                \r\n            </mt-tab-container>\r\n            <!-- 底部选项卡 -->\r\n            <mt-tabbar v-model=\"active\" fixed  >\r\n                <mt-tab-item id=\"message\" @click.native=\"changeState(0)\" >\r\n                    <tabbaricon :focused=\"currentIndex[0].isSelect\" :selectedImg=\"require('../../assets/ic_weixin_selected.png')\" :normalImg=\"require('../../assets/ic_weixin_normal.png')\"></tabbaricon>  \r\n                    消息\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"contact\" @click.native=\"changeState(1)\">\r\n                    <tabbaricon :focused=\"currentIndex[1].isSelect\" :selectedImg=\"require('../../assets/ic_me_selected.png')\" :normalImg=\"require('../../assets/ic_me_normal.png')\"></tabbaricon>\r\n                    通讯录\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"find\" @click.native=\"changeState(2)\">\r\n                    <tabbaricon   :focused=\"currentIndex[2].isSelect\" :selectedImg=\"require('../../assets/ic_find_selected.png')\" :normalImg=\"require('../../assets/ic_find_normal.png')\"></tabbaricon>\r\n                    发现\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"me\" @click.native=\"changeState(3)\" >\r\n                    <tabbaricon  :focused=\"currentIndex[3].isSelect\" :selectedImg=\"require('../../assets/ic_me_selected.png')\" :normalImg=\"require('../../assets/ic_me_normal.png')\"></tabbaricon>\r\n                    我\r\n                </mt-tab-item>       \r\n            </mt-tabbar>   \r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n//1.引入子组件\r\nimport TitleBar from \"./common/TitleBar\"\r\nimport messagelist from \"./common/MessageList\"\r\nimport tabbaricon from \"./common/TabBaricon\"\r\nexport default {\r\n    data(){\r\n        return {\r\n            leftTitle:\"11\",   //标题\r\n            rightFirstImg:require(\"../../assets/ic_search.png\"),  //图片+\r\n            rightSecondImg:require(\"../../assets/ic_add.png\"),    //图片search\r\n            active:\"message\",\r\n            currentIndex:[\r\n                {isSelect:true},\r\n                {isSelect:false},\r\n                {isSelect:false},\r\n                {isSelect:false}\r\n                ]       \r\n        }\r\n    },\r\n    components:{\r\n        \"titlebar\":TitleBar,                //2.注册子组件\r\n        messagelist,\r\n        tabbaricon       \r\n    },\r\n    methods:{\r\n        add(){\r\n            console.log(\"+\")\r\n        },\r\n        search(){\r\n            console.log(\"搜索\")\r\n        },\r\n       changeState(n){\r\n           for(var i=0;i<this.currentIndex.length;i++){\r\n               if(n==i){\r\n                   this.currentIndex[i].isSelect=true;\r\n               }else{\r\n                   this.currentIndex[i].isSelect=false;\r\n               }\r\n           }\r\n       }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n/* // 最外层父元素溢出隐藏 */\r\n    .page-tabbar{\r\n        overflow: hidden;\r\n    }\r\n    .page-wrap{\r\n        overflow: auto;\r\n        padding-bottom: 60px;\r\n    }\r\n    /* 修改tabbar 默认文字颜色 */\r\n    .mint-tabbar>.mint-tab-item{\r\n        color:#999999;\r\n    }\r\n    .mint-tabbar > .mint-tab-item.is-selected {\r\n        background-color:transparent;\r\n        color: #45c018;\r\n    }\r\n</style>\r\n"]}]}