div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      uname:\"\",\r\n      upass:\"\"\r\n    }\r\n  },\r\n  methods:{\r\n    login(){  //完成用户登录\r\n      //获取用户输入的用户名\r\n      var uname=this.uname;\r\n      //获取用户输入的密码\r\n      \r\n       var upass=this.upass\r\n      //创建二个正则表达式\r\n      var nreg=/^\\w{3,10}$/\r\n      var preg=/^\\w{3,8}$/\r\n       //判断用户名格式是否正确\r\n      if(!nreg.test(uname)){\r\n        this.$toast(\"用户名格式错误\");\r\n        return ;\r\n      }                \r\n      //判断用户密码是否正确\r\n      if(!preg.test(upass)){\r\n        this.$toast(\"密码格式不正确\");\r\n        return ;\r\n      }\r\n     \r\n      //发送ajax请求\r\n     \r\n      this.axios.get(\"login\",{params:{uname,upass}}).then(result=>{\r\n        if(result.data.code==1){\r\n          this.$router.push(\"/cart\");\r\n        }else{\r\n          this.$messagebox(\"消息\",`${result.data.msg}`)\r\n        }    \r\n      }\r\n      )\r\n      // 获取服务器返回结果\r\n      // 返回失败 提示\r\n      //返回成功 跳转到home.vue\r\n    } \r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n    .center{\r\n        text-align: center;    \r\n    }\r\n   input[type='password']{\r\n        border:0!important;\r\n        outline: 0!important;\r\n    }\r\n    .btn{\r\n        background: #ff4400;\r\n        color: #fff;\r\n        display: block;\r\n        width: 90%;\r\n        height: 50px;\r\n        margin: 0 0px 0 21px; \r\n        border-radius: 30px;\r\n    }\r\n</style>\r\n"]}]}